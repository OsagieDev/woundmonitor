
	<div class="row-fluid wa-ctr-text">
		<h1>Welcome to Wound Assist+</h1>

		<hr>

		<h3>Staff Login</h3>

		<%= form_for @user, url: sessions_path do |f| %>

			<%= f.email_field :email, placeholder: "Email" %><br />
			<%= f.password_field :password, placeholder: "Password" %><br />
			<%= f.submit "Login", :class => "btn btn-hg btn-primary" %>
		<% end %>


		<hr>
	</div>


